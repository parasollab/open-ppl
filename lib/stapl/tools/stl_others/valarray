#ifndef STAPL_STL_OTHERS_VALARRAY_H
#define STAPL_STL_OTHERS_VALARRAY_H

// supports std::valarray<> packing through Boost.Serialization
#include "stl_location.h"
#include STAPL_STL_INCLUDE(valarray)
#if !defined(STD)
# include <boost/serialization/vector.hpp>
# include <boost/serialization/valarray.hpp>
#endif

#endif

