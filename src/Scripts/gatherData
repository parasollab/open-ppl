echo $1
cd $1
echo "RRT"
find -name RRT.*.stat | xargs ../filter $2 | xargs grep "Number of Nodes" | xargs ../avg Nodes 4 0
find -name RRT.*.stat | xargs ../filter $2 | xargs grep "Total Cfg" | xargs ../avg CD 4 0
find -name RRT.*.stat | xargs ../filter $2 | xargs grep "RRT Generation:" | xargs ../avg Time 4 1
