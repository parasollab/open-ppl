  <!-- Here we specify input files, such as environment and query. The
       'baseFilename in the Problem node is for Simulator stat output only. -->
  <Problem baseFilename="MultiRobotExample">

    <Environment filename="../Envs/3D/mr-3d.xml"/>

    <Robot  label="boxy-0" filename="../Envs/3D/iCreate.xml">
      <Agent type="child" waypointDm="euclidean" waypointThreshold=".05" debug="true"/>
	  </Robot>
	  <Robot  label="boxy-1" filename="../Envs/3D/iCreate.xml">
      <Agent type="child" waypointDm="euclidean" waypointThreshold=".05" debug="true"/>
	  </Robot>
	  <Robot  label="boxy-2" filename="../Envs/3D/iCreate.xml">
      <Agent type="child" waypointDm="euclidean" waypointThreshold=".05" debug="true"/>
	  </Robot>
	  <Robot  label="boxy-3" filename="../Envs/3D/iCreate.xml">
      <Agent type="child" waypointDm="euclidean" waypointThreshold=".05" debug="true"/>
	  </Robot>
	  <Robot  label="boxy-4" filename="../Envs/3D/iCreate.xml">
      <Agent type="child" waypointDm="euclidean" waypointThreshold=".05" debug="true"/>
	  </Robot>
	  <Robot  label="boxy-5" filename="../Envs/3D/iCreate.xml">
      <Agent type="child" waypointDm="euclidean" waypointThreshold=".05" debug="true"/>
	  </Robot>
	  <Robot  label="boxy-6" filename="../Envs/3D/iCreate.xml">
      <Agent type="child" waypointDm="euclidean" waypointThreshold=".05" debug="true"/>
	  </Robot>
	  <Robot  label="boxy-7" filename="../Envs/3D/iCreate.xml">
      <Agent type="child" waypointDm="euclidean" waypointThreshold=".05" debug="true"/>
	  </Robot>

    <!-- The robotLabels parameter lists all of the robots included in this group -->
	  <RobotGroup label="robotGroup" robotLabels="
		  boxy-0 boxy-1 boxy-2 boxy-3 boxy-4
	    boxy-5 boxy-6 boxy-7 "/>


