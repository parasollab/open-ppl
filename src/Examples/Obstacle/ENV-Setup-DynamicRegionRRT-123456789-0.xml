<MotionPlanning print="false" warnings="true" warningsAsErrors="true">

  <Problem baseFilename="example">

    <Environment filename="obstacle.env" frictionCoefficient="0" gravity="0 0 0" />

    <Robot filename="robot.robot" label="boxy">
      
      <Agent type="pathfollowing" waypointDm="euclideanPosition" waypointThreshold=".05" />
    </Robot>

    <Task label="query" robot="boxy">
      <StartConstraints>
        
        <CSpaceConstraint point="-36 -18 0 0 0 0" />
      </StartConstraints>

      <GoalConstraints>
        
        <CSpaceConstraint point="-8 14 0 0 0 0" />
      </GoalConstraints>
    </Task>

  </Problem>

  
  <Library>

    <DistanceMetrics>
      <Euclidean label="euclidean" />
      <ScaledEuclidean label="scaledEuclidean" scale="0.5" />
      <WeightedEuclidean avlWeight="0" label="euclideanPosition" posWeight="1" rotWeight="0" velWeight="0" />
      <CenterOfMass label="com" />
      <KnotTheory label="knot" />
      <Manhattan label="manhattan" />
      <Minkowski label="minkowski" normalize="false" r1="3.0" r2="3.0" r3="0.333" />
      <RMSD label="rmsd" />
      <LPSwept label="LPSwept" lpLabel="sl" oriRes="5" posRes="5" useBBox="true" />
      <BinaryLPSwept label="BLPSwept" lpLabel="sl" oriRes="5" posRes="5" useBBox="true" />
      <TopologicalDistance label="connectedFreeSpace" operator="sum" tmLabel="tm-coarse" />
    </DistanceMetrics>

    <ValidityCheckers>
      <CollisionDetection label="rapid" method="RAPID" />
      <CollisionDetection label="pqp" method="PQP" />
      <CollisionDetection label="pqp_solid" method="PQP_SOLID" />
      <NodeClearanceValidity delta="0.3" label="nc" nfLabel="Nearest" />
      <NegateValidity label="nv" vcLabel="rapid" />
      <MedialAxisClearance clearanceType="exact" dmLabel="euclidean" label="mac" maClearance="0.5" penetrationType="exact" positional="true" useBBX="true" vcLabel="pqp_solid" />
      <ComposeValidity label="cv" operator="AND">
        <ValidityChecker label="pqp_solid" />
        <ValidityChecker label="mac" />
      </ComposeValidity>
      <AlwaysTrueValidity label="alwaysTrue" />
      <ObstacleClearance clearanceThreshold="1" label="oc" vcLabel="pqp_solid" />
    </ValidityCheckers>

    <NeighborhoodFinders>
      <BruteForceNF dmLabel="euclidean" k="10" label="BFNF" unconnected="false" />
      <BruteForceNF dmLabel="euclidean" k="1" label="Nearest" unconnected="false" />
      <BruteForceNF dmLabel="euclidean" k="0" label="BFNFAll" />
      <HierarchicalNF dmLabel="" label="HierarchicalNF" nfLabel="BFNF" nfLabel2="RandomNF" />
      <HopLimitNF dmLabel="" hoplimit="2" label="HLNF" nfLabel="BFNF" />
      <OptimalNF label="OptimalK" nfLabel="BFNF" />
      <RadiusNF dmLabel="euclidean" label="RadiusNF" radius="10" />
      <RandomNF dmLabel="euclidean" k="5" label="RandomNF" />
      <DPESNF dmLabel="euclidean" k="5" label="DPESNF" m="3" />
      
      <BandsNF debug="false" dmLabel="euclidean" label="RbandNF_2bands_4k">
        <RBand dmLabel="euclidean" k="8" label="first" max="8" policy="closest" />
        <RBand dmLabel="euclidean" k="8" label="second" max="64" min="8" policy="random" />
      </BandsNF>
      <BandsNF debug="false" dmLabel="euclidean" label="DbandNF">
        <DBand dmLabel="euclidean" k="16" label="first" max=".3" min=".1" policy="closest" />
      </BandsNF>
      <TopologicalFilter backtrackDistance="6" debug="false" label="TopologicalNF" nfLabel="BFNF" tmLabel="tm-coarse" />
      <LSHNF dmLabel="euclidean" hashDimension="3" hashFamilies="8" hashRadius="9" k="8" label="LSHNF" />
      <KdTreeNF dmLabel="euclidean" epsilon=".01" k="5" label="KdTreeNF" useScaling="true" />
    </NeighborhoodFinders>

    <Samplers>
      <UniformRandomSampler label="UniformRandom" vcLabel="alwaysTrue" />
      <UniformRandomSampler label="UniformRandomFree" vcLabel="pqp_solid" />
      <ObstacleBasedSampler debug="false" dmLabel="euclidean" label="OBPRM" nShellsColl="0" nShellsFree="1" pointSelection="cspace" stepSize="0.0" useBBX="false" vcLabel="rapid" />
      <GaussianSampler d="0.2" dmLabel="euclidean" label="Gauss" useBBX="false" vcLabel="rapid" />
      <BridgeTestSampler d="0.2" dmLabel="euclidean" label="Bridge" useBBX="true" vcLabel="rapid" />
      <MedialAxisSampler clearanceType="exact" debug="false" dmLabel="euclidean" label="MAPRM" penetrationType="exact" positional="true" useBBX="true" vcLabel="pqp_solid" />
      <GridSampler debug="false" label="Grid" ticks="10 10 10" vcLabel="pqp_solid" />
      <MixSampler label="MixSampler">
        <Sampler label="UniformRandomFree" p="0.8" />
        <Sampler label="UniformRandom" p="0.2" />
      </MixSampler>
      <UniformObstacleBasedSampler d="0.02" dmLabel="euclidean" label="UOBPRM" useBBX="false" vcLabel="rapid" />
      <UniformMedialAxisSampler clearanceType="exact" d="2.00" debug="false" dmLabel="euclidean" label="UMAPRM" penetrationType="exact" positional="true" t="10" useBBX="true" vcLabel="pqp_solid" />
      <WorkspaceImportanceSampler alpha="0.2" decompositionLabel="fine" label="WIS" vcLabel="rapid" />
      <DynamicRegionSampler biasMetric="default" clearanceValue="max" debug="true" decompositionLabel="fine" explore=".2" label="DynamicRegionSampler" penetration="1" prune="false" regionFactor="2.5" samplerLabel="UniformRandomFree" skeletonIO="write" skeletonType="reeb" space="1" velocityAlignment=".4" velocityBiasing="false" />
    </Samplers>

    <LocalPlanners>
      <StraightLine binaryEvaluation="true" label="sl" vcLabel="rapid" />
      <StraightLine binaryEvaluation="true" label="slAlwaysTrue" vcLabel="alwaysTrue" />
      <RotateAtS binaryEvaluation="true" label="ras" s="0.5" saveIntermediates="false" vcLabel="rapid" />
      <TransformAtS binaryEvaluation="true" label="tas" s="0.5" saveIntermediates="false" vcLabel="rapid" />
      <AStarDistance debug="true" dmLabel="euclidean" histLength="10" label="asd" maxTries="6" numNeighbors="10" vcLabel="rapid" />
      <AStarClearance clearanceType="exact" debug="false" dmLabel="euclidean" histLength="10" label="asc" maxTries="6" numNeighbors="3" penetrationType="exact" positional="true" useBBX="true" vcLabel="pqp_solid" />
      <MedialAxisLP clearanceType="exact" controller="recursive" debug="true" dmLabel="euclidean" label="malpr" macEpsilon="0.1" maxIter="10" penetrationType="exact" positional="true" saveIntermediates="true" useBBX="true" vcLabel="pqp_solid" />
      <MedialAxisLP clearanceType="exact" controller="iterative" debug="true" dmLabel="euclidean" label="malpi" maxFailures="100" maxIter="100" noProgress="1" pathModifier="ShortcuttingPathModifier" penetrationType="exact" positional="true" resFactor="20" saveIntermediates="true" useBBX="true" vcLabel="pqp_solid" />
      <MedialAxisLP clearanceType="exact" controller="binary" debug="true" dmLabel="euclidean" label="malpb" maxIter="100" penetrationType="exact" positional="true" resFactor="20" saveIntermediates="true" useBBX="true" vcLabel="pqp_solid" />
      <ToggleLP dmLabel="euclidean" label="togglelp" lpLabel="sl" maxIter="5" vcLabel="rapid" />
      <ApproxSpheres clearanceType="exact" dmLabel="euclidean" label="apsph" penetrationType="exact" saveIntermediates="false" vcLabel="pqp_solid" />
      <HierarchicalLP label="hlp">
        <LocalPlanner method="sl" />
        <LocalPlanner method="ras" />
        <LocalPlanner method="malp" />
      </HierarchicalLP>
    </LocalPlanners>

    <Extenders>
      <BasicExtender debug="false" dmLabel="euclidean" label="BERO" maxDist="4." minDist=".01" vcLabel="rapid" />
      <BasicExtender debug="false" dmLabel="euclidean" label="BESO" maxDist="10." minDist=".001" randomOrientation="false" vcLabel="rapid" />
      <KinodynamicExtender best="true" debug="false" dmLabel="euclidean" fixed="false" label="KinoExtender" minDist=".01" numSteps="10" vcLabel="rapid" />
      <MedialAxisExtender clearanceType="exact" dmLabel="euclidean" extendDist="0.5" label="MAE" lpLabel="sl" maxDist="10.0" maxIntermediates="10" minDist="0.01" penetrationType="exact" positional="true" useBBX="true" vcLabel="pqp_solid" />
      <RandomObstacleVector debug="false" dmLabel="euclidean" label="ROVRO" maxDist="10.0" minDist=".001" vcLabel="rapid" />
      <RandomObstacleVector debug="false" dmLabel="euclidean" label="ROVSO" maxDist="10.0" minDist=".001" randomOrientation="false" vcLabel="rapid" />
      <RotationThenTranslation debug="false" dmLabel="euclidean" label="RTT" maxDist="10.0" minDist=".001" vcLabel="rapid" />
      <TraceCSpaceObstacle debug="false" dmLabel="euclidean" label="TCSO" maxDist="10.0" minDist=".001" vcLabel="rapid" />
      <TraceMAPush clearanceType="exact" debug="false" dmLabel="euclidean" label="TMAP" maxDist="10.0" minDist=".001" penetrationType="exact" vcLabel="rapid" />
      <TraceObstacle debug="false" dmLabel="euclidean" label="TORO" maxDist="10.0" minDist=".001" vcLabel="rapid" />
      <TraceObstacle debug="false" dmLabel="euclidean" label="TOSO" maxDist="10.0" minDist=".001" randomOrientation="false" vcLabel="rapid" />
      <MixExtender label="ME">
        <Extender label="BERO" probability="0.1" />
        <Extender label="BESO" probability="0.1" />
        <Extender label="ROVRO" probability="0.1" />
        <Extender label="ROVSO" probability="0.1" />
        <Extender label="RTT" probability="0.1" />
        <Extender label="TORO" probability="0.1" />
        <Extender label="TOSO" probability="0.1" />
        <Extender label="TCSO" probability="0.1" />
        <Extender label="TMAP" probability="0.1" />
      </MixExtender>
    </Extenders>

    <PathModifiers>
      <CombinedPathModifier debug="true" label="CombinedPathModifier">
        <Modifier label="ShortcuttingPathModifier" />
        <Modifier label="MedialAxisPathModifier" />
        <Modifier label="ResamplePathModifier" />
      </CombinedPathModifier>
      <CRetractionPathModifier clearanceType="exact" debug="false" dmLabel="euclidean" iter="25" label="CR" penetrationType="exact" positional="true" useBBX="true" vcLabel="pqp_solid" />
      <MedialAxisPathModifier debug="false" label="MAPS" lpLabel="sl" malpLabel="malpi" pmLabel="SC" />
      <ResamplePathModifier clearanceType="exact" debug="false" dmLabel="euclidean" label="ResamplePathModifier" lpLabel="sl" numResamples="1" penetrationType="exact" positional="true" stepSize="0.3" typeName="MAX_CLEARANCE" useBBX="true" userValue="0.3" vcLabel="pqp_solid" />
      <ShortcuttingPathModifier debug="false" label="ShortcuttingPathModifier" lpLabel="sl" />
    </PathModifiers>

    <Connectors>
      <AdaptiveConnector checkIfSameCC="false" debug="false" fixedCost="false" fixedReward="false" label="AdaptiveCS" lpLabel="sl" nfLabel="" percentRandom="0.5" uniformProbability="false">
        <NeighborFinder Method="BFNF" />
        <NeighborFinder Method="Nearest" />
        <NeighborFinder Method="RandomNF" />
        <NeighborFinder Method="BFNFAll" />
      </AdaptiveConnector>
      <CCsConnector k="5" label="ConnectCCs" lpLabel="sl" nfLabel="BFNF" />
      <NeighborhoodConnector checkIfSameCC="false" debug="false" label="Closest" lpLabel="sl" nfLabel="BFNF" />
      <NeighborhoodConnector checkIfSameCC="true" label="SparkPRMConnector" lpLabel="sl" nfLabel="Nearest" />
      <NeighborhoodConnector checkIfSameCC="false" label="ToggleConnect" lpLabel="sl" maxFailures="1" nfLabel="BFNF" />
      <NeighborhoodConnector checkIfSameCC="false" label="ClosestAlwaysTrue" lpLabel="slAlwaysTrue" nfLabel="BFNF" />
      <NeighborhoodConnector checkIfSameCC="false" label="AllToAll" lpLabel="sl" nfLabel="BFNFAll" />
      <NeighborhoodConnector checkIfSameCC="false" label="KdTree" lpLabel="sl" nfLabel="KdTreeNF" />
      <RewireConnector label="OptimalRewire" lpLabel="sl" nfLabel="OptimalK" />
    </Connectors>

    <Metrics>
      <NumNodesMetric label="NumNodes" />
      <NumEdgesMetric label="NumEdges" />
      
      <CoverageMetricVectorSet computeAllCCs="false" filename="coverage" label="CoverageVectorSet">
        <Connector label="Closest" />
      </CoverageMetricVectorSet>
      <ConnectivityMetricVectorSet filename="connectivity" label="ConnectivityVectorSet">
        <Connector label="Closest" />
      </ConnectivityMetricVectorSet>
      <DiameterMetric label="Diameter" />
      <CCDistanceMetric dmLabel="euclidean" label="CCDistance" />
      <TimeMetric label="TimeInSec" />
    </Metrics>

    <MapEvaluators>
      <TrueEvaluation label="TrueEvaluator" />
      <QueryMethod debug="false" graphSearchAlg="dijkstras" label="Query" />
      <LazyQuery d="0.5" debug="false" deleteInvalid="true" enhanceDmLabel="euclidean" graphSearchAlg="dijkstras" label="LazyQuery" lpLabel="sl" numEnhance="10" vcLabel="pqp_solid">
        <NodeConnectionMethod label="ClosestAlwaysTrue" />
        <Resolution mult="27" />
        <Resolution mult="16" />
        <Resolution mult="1" />
      </LazyQuery>
      <LazyToggleQuery debug="false" dmLabel="euclidean" graphSearchAlg="dijkstras" iterative="true" label="LazyToggleQuery" lpLabel="sl" vcLabel="rapid">
        <NodeConnectionMethod label="ClosestAlwaysTrue" />
        <ToggleConnectionMethod label="LazyToggleConnect" />
        <Resolution mult="27" />
        <Resolution mult="16" />
        <Resolution mult="1" />
      </LazyToggleQuery>
      <PrintMapEvaluation base_name="Basic" label="PrintMap" />
      <ConditionalEvaluator label="NodesEval" metric_method="NumNodes" operator="&gt;=" value="5000" />
      <ConditionalEvaluator label="EdgesEval" metric_method="NumEdges" operator="&gt;" value="1000" />
      <ConditionalEvaluator label="FreqNodeEval" metric_method="NumNodes" operator="%" value="50" />
      <NegateEvaluator evalLabel="NodesEval" label="Neg" />
      <ComposeEvaluator label="BoundedQuery" operator="or">
        <Evaluator label="NodesEval" />
        <Evaluator label="Query" />
      </ComposeEvaluator>
      <TimeEvaluator label="TimeEval" timeout="10" />
    </MapEvaluators>

    
    

    <MPStrategies>

      
      <BasicPRM label="BasicPRM1">
        <Sampler attempts="1" label="UniformRandomFree" number="1" />
        
        
        
        <Connector label="Closest" />
        
        
        <Evaluator label="Query" />
      </BasicPRM>

      
      <BasicPRM inputMap="input.map" label="BasicPRM2" startAt="connecting">
        <Sampler attempts="10" label="UniformRandomFree" number="10" />
        <Connector label="Closest" />
        <ComponentConnector label="ConnectCCs" />
        <Evaluator label="NodesEval" />
      </BasicPRM>

      
      <BasicRRTStrategy debug="true" extenderLabel="BERO" goalDmLabel="euclidean" goalThreshold="5" growGoals="false" growthFocus="0.01" label="RRT" m="1" nfLabel="Nearest" querySampler="UniformRandomFree" samplerLabel="UniformRandom">
        <Evaluator label="Query" />
      </BasicRRTStrategy>

      
      <BasicRRTStrategy debug="false" extenderLabel="KinoExtender" goalDmLabel="euclidean" goalThreshold="6" growGoals="false" growthFocus="0.01" label="KinoRRT" nfLabel="Nearest" querySampler="UniformRandomFree" samplerLabel="UniformRandom">
        <Evaluator label="Query" />
      </BasicRRTStrategy>

      
      <BasicRRTStrategy connectorLabel="OptimalConnector" extenderLabel="BERO" growGoals="false" growthFocus="0.01" label="RRG" nfLabel="Nearest" querySampler="UniformRandomFree" samplerLabel="UniformRandom">
        <Evaluator label="NodesEval" />
      </BasicRRTStrategy>

      
      <BasicRRTStrategy connectorLabel="OptimalRewire" extenderLabel="BERO" growGoals="false" growthFocus="0.01" label="RRTstar" nfLabel="Nearest" querySampler="UniformRandomFree" samplerLabel="UniformRandom">
        <Evaluator label="NodesEval" />
      </BasicRRTStrategy>

      <SRTStrategy closeCent="1" closePairs="2" connIter="5" debug="false" exLabel="BERO" expansions="10" label="SRT" lpLabel="sl" nfLabel="Nearest" randCent="2" samplerLabel="UniformRandomFree" samples="5">
        <Evaluator label="Query" />
      </SRTStrategy>

      
      
      <BasicRRTStrategy debug="false" extenderLabel="ME" growGoals="true" label="OBRRT" nfLabel="Nearest" querySampler="UniformRandomFree" samplerLabel="UniformRandom">
        <Evaluator label="NodesEval" />
      </BasicRRTStrategy>

      
      

      
      

      
      <BasicRRTStrategy debug="false" extenderLabel="BERO" goalDmLabel="euclideanPosition" growGoals="false" growthFocus="0.05" label="DynamicRegionRRT" nfLabel="Nearest" querySampler="UniformRandomFree" samplerLabel="DynamicRegionSampler">
        <Evaluator label="BoundedQuery" />
      </BasicRRTStrategy>

      
      <TogglePRMStrategy label="TogglePRM" priority="true">
        <Sampler attempts="1" label="UniformRandomFree" number="10" />
        <Connector label="Closest" />
        <ColConnector label="ToggleConnect" />
        <Evaluator label="Query" />
      </TogglePRMStrategy>

      
      

      
      <EvaluateMapStrategy label="EvaluateMapStrategy">
        <Evaluator label="NodesEval" />
      </EvaluateMapStrategy>

      <UtilityGuidedGenerator connectorLabel="Closest" label="UtilityGuided" nfLabel="BFNF" vcLabel="rapid">
        <Evaluator label="NodesEval" />
      </UtilityGuidedGenerator>

      
      <PushQueryToMA clearanceType="exact" dmLabel="euclidean" inFilename="3D/3d.query" label="PushQueryToMA" outFilename="3D/3d.ma.query" penetrationType="exact" positional="true" useBBX="true" vcLabel="pqp_solid" />

      
      <ModifyPath label="ModifyPath" pathFile="3D/3d.path" pmLabel="CR" />

      
      <LPCompare dmLabel="euclidean" label="LPCompare" lpLabel1="sl" lpLabel2="malpi" rdmp1="Map1.map" rdmp2="Map2.map" />

      
      <MultiStrategy label="MultiStrategy">
        <MPStrategy label="BasicPRM1" />
        <MPStrategy label="AdaptiveRRT" />
      </MultiStrategy>

      
      <SparkTogglePRMStrategy attemptRatio="10" biasConnect="true" cLabel="SparkPRMConnector" checkEdgeCases="true" checkImportant="true" delta="5" dmLabel="euclidean" eLabel="BERO" growthFocus="0.25" initSamples="10" label="SparkToggle" maxNPCCSize="2" maxRRTSize="100" minDist="0.1" nfLabel="BFNF" nfVertexLabel="Nearest" priority="false" rrtDebug="false" trimAll="true" trimDepth="2" vcLabel="rapid">
        <Sampler attempts="10" label="UniformRandomFree" number="10" />
        <Connector label="ToggleConnect" />
        <ColConnector label="ToggleConnect" />
        <Evaluator label="Query" />
      </SparkTogglePRMStrategy>

      <SparkBasicPRM attemptRatio="10" biasConnect="true" cLabel="SparkPRMConnector" checkEdgeCases="true" checkImportant="true" delta="5" dmLabel="euclidean" eLabel="BERO" growthFocus="0.25" initSamples="10" label="SparkUniform" maxNPCCSize="2" maxRRTSize="100" minDist="0.1" nfLabel="BFNF" nfVertexLabel="Nearest" rrtDebug="false" trimAll="true" trimDepth="2" vcLabel="rapid">
        <Sampler attempts="10" label="UniformRandomFree" number="10" />
        <Connector label="Closest" />
        <Evaluator label="Query" />
      </SparkBasicPRM>

      
      <Syclop debug="false" extenderLabel="BERO" goalDmLabel="euclideanPosition" goalThreshold="4" growGoals="false" growthFocus="0.01" label="Syclop" nfLabel="Nearest" querySampler="UniformRandomFree" samplerLabel="UniformRandom" tmLabel="tm-fine">
        <Evaluator label="Query" />
      </Syclop>

      
      <StableSparseRRT debug="true" extenderLabel="BERO" goalDmLabel="euclideanPosition" goalThreshold="4" growthFocus="0.01" label="SST" nfLabel="RadiusNF" prune="true" querySampler="UniformRandomFree" samplerLabel="UniformRandom" witnessNfLabel="Nearest" witnessRadius=".5">
        <Evaluator label="BoundedQuery" />
      </StableSparseRRT>

      
      <VisibilityBasedPRM debug="false" label="VBPRM" lpLabel="sl" maxFailedIterations="20" sampler="UniformRandomFree" vcLabel="rapid" />

      <ScratchStrategy label="scratch" />

    </MPStrategies>

    <MPTools>
      

      
      <TetGenDecomposition baseFilename="coarse" debug="false" io="write" label="coarse" switches="pnQ" />
      <TetGenDecomposition baseFilename="quality" debug="false" io="write" label="fine" switches="pnqQ" />

      
      <TopologicalMap decompositionLabel="coarse" gridSize="1" label="tm-coarse" />
      <TopologicalMap decompositionLabel="fine" gridSize="1" label="tm-fine" />

      <ReebGraphConstruction debug="false" filename="" write="false" />

      <ClearanceUtility clearanceType="exact" dmLabel="euclidean" label="clearanceUtil" penetrationType="exact" positional="true" useBBX="true" vcLabel="pqp_solid" />

      <MedialAxisUtility clearanceType="exact" dmLabel="euclidean" label="maUtil" penetrationType="exact" positional="true" useBBX="true" vcLabel="pqp_solid" />

      <SVMModel c="10" epsilon=".001" gamma="1" kernel="radial" />

    </MPTools>

    

    <Solver baseFilename="TR-DynamicRegionRRT-0" mpStrategyLabel="DynamicRegionRRT" seed="123456789" vizmoDebug="true" />

  </Library>

</MotionPlanning>