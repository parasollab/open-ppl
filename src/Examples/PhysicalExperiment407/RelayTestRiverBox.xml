        connectorLabel="OptimalRewire" gtype="UNDIRECTED_GRAPH"
        samplerLabel="UniformRandom">
        <Evaluator label="NodesEval"/>
      </BasicRRTStrategy>

      <SRTStrategy label="SRT"
        lpLabel="sl" dmLabel="euclidean" nfLabel="Nearest"
